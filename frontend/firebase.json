{
  "hosting": {
    "public": "frontend/build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "/api/**",
        "destination": "http://localhost:5000"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}